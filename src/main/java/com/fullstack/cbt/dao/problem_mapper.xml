<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fullstack.cbt.dao.ProblemDAO">

	<select id="subjectList" resultType="String">
		SELECT su_name FROM cbt_subject WHERE su_is_blind = 0
	</select>
	
	<select id="chapList" resultType="String">
		SELECT sc_name FROM cbt_subject_chapter WHERE su_idx = #{param1}
	</select>

</mapper>